	for (var i = 0; i < ab.length; i++) {
		var option = document.createElement("OPTION"),
			txt = document.createTextNode(ab[i]);
		option.appendChild(txt);
		option.setAttribute("value", ab[i]);
		id_child2.insertBefore(option, id_child2.lastChild);
	}


}


