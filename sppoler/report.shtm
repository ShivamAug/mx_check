    var x = document.getElementById("Start").value;
    var y = document.getElementById("End").value;

    // Push column data into data list

    for (var i = 1; i < lines.length - 1; i++) {

      var cat = lines[i].split(",")[0];
   

      for (var j = 1; j <= 1; j++) {

        var fd;
        var fm;
        var td;
        var tm;
        var cd;
        var cm;
        var str = x;
        var str1 = y;
        var str2 = cat;

     var arrayf = str.split("/", 1);
        var arrayf1 = str.split("/", 2);
